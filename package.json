{
  "name": "cypress_test_framework",
  "private": true,
  "description": "Cypress test Framework",
  "author": "Dipjyoti Metia",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf cypress/reports",
    "lint": "tslint ./src/**/*.js -t verbose",
    "combine-reports": "mochawesome-merge ./cypress/reports/mocha/*.json > mochareports/mochawesome.json",
    "generate-report": "marge mochareports/mochawesome.json",
    "cypress:open": "cypress open",
    "start:ci": "my-server -p 3030",
    "cy:verify": "cypress verify",
    "cy:run": "cypress run --headed --spec 'cypress/integration/example_spec.ts'",
    "firefox:run": "cypress run --headed --browser firefox --spec 'cypress/integration/example_spec.ts'",
    "chrome:run": "cypress run --headed --browser chrome --spec 'cypress/integration/example_spec.ts'",
    "edge:run": "cypress run --headed --browser edge --spec 'cypress/integration/example_spec.ts'",
    "test": "percy exec -- cypress run --spec 'cypress/integration/example_spec.ts'",
    "test:ci": "start-server-and-test start:ci http://localhost:3030 cy:run",
    "test:cypress": "cross-env CYPRESS_API_URL=http://localhost:1234 cy2 run --record --key vfepbj --parallel --ci-build-id cypress-test"
  },
  "keywords": [
    "node",
    "cypress",
    "test",
    "typescript"
  ],
  "dependencies": {
    "axe-core": "^4.4.1",
    "chalk": "^5.0.0",
    "cross-env": "^7.0.3",
    "cy2": "^1.3.0",
    "cypress": "^9.4.1",
    "cypress-axe": "^0.14.0",
    "cypress-downloadfile": "^1.2.1",
    "date-fns": "^2.28.0",
    "faker": "^6.6.6",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cypress/skip-test": "^2.6.1",
    "@neuralegion/cypress-har-generator": "^5.3.0",
    "@percy/cli": "1.0.0-beta.75",
    "@percy/cypress": "^3.1.1",
    "@testing-library/cypress": "^8.0.2",
    "@types/chai": "^4.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jquery": "^3.5.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.16",
    "concurrently": "^7.0.0",
    "cross-env": "7.0.3",
    "cypress-audit": "^1.1.0",
    "cypress-dark": "^1.8.3",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-ntlm-auth": "^3.2.4",
    "cypress-parallel": "^0.9.0",
    "cypress-xpath": "^1.6.2",
    "eslint-plugin-cypress": "^2.12.1",
    "fs-extra": "^10.0.0",
    "knex": "^1.0.3",
    "lighthouse": "^9.3.1",
    "mocha": "^9.2.0",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.0.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.5"
  }
}
